{
  "name": "urico-kansai",
  "type": "module",
  "version": "1.0.0",
  "description": "購入希望者情報自動生成システム",
  "scripts": {
    "start": "npx serve . -l 3000",
    "start:static": "npx serve public -l 3000",
    "dev": "npx serve . -l 3000",
    "generate:jobs": "node scripts/generate_jobs.cjs",
    "generate:run": "node scripts/run_parallel.cjs",
    "generate:run:10": "node scripts/run_parallel.cjs 10",
    "generate:run:prefecture": "./scripts/run_by_prefecture.sh",
    "generate:convert": "node scripts/convert_to_js.cjs",
    "generate:update": "node scripts/update_main_files.cjs",
    "generate:all": "npm run generate:jobs && npm run generate:run && npm run generate:convert && npm run generate:update",
    "generate:static": "node scripts/generate_static_pages.mjs",
    "generate:static:incremental": "node scripts/generate_static_incremental.mjs",
    "generate:static:by-pref": "./scripts/generate_by_prefecture.sh",
    "generate:faq": "node scripts/generate-urico-faq-from-ikura.mjs",
    "crawl:ikura": "USE_SAMPLE_DATA=true node scripts/crawl-ikura-pages.mjs",
    "crawl:ikura:real": "node scripts/crawl-ikura-pages.mjs",
    "build": "./scripts/generate_by_prefecture.sh"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "main": "main.js",
  "directories": {
    "lib": "lib"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/c21-japan/urico-japan.git"
  },
  "bugs": {
    "url": "https://github.com/c21-japan/urico-japan/issues"
  },
  "homepage": "https://github.com/c21-japan/urico-japan#readme",
  "dependencies": {
    "@google/generative-ai": "^0.24.1"
  },
  "devDependencies": {
    "cheerio": "^1.2.0",
    "p-limit": "^7.2.0",
    "undici": "^7.19.1",
    "wrangler": "^4.60.0"
  }
}
